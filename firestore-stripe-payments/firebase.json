      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "runtime": "nodejs16"
  },
  "emulators": {
    "functions": {
      "port": 5001,
      "runtime": "nodejs16"
    },
    "ui": {
      "enabled": true
    },
